                                    android:layout_height="wrap_content"
                                    android:paddingLeft="4dp"
                                    android:paddingRight="4dp"
                                    android:textStyle="bold"
                                    android:layout_gravity="center_vertical"
                                    android:textSize="@dimen/text_size"
                                    android:text="@string/colon"/>
                                <LinearLayout
