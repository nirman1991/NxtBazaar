<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="in.co.retail.nxtbazzar.UserActivity">
    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/app_background"
        android:layout_marginTop="?attr/actionBarSize">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/deliveryCaption"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_gravity="center_horizontal"
                android:textSize="20dp"
                android:textStyle="bold|italic"
                android:textColor="@android:color/holo_red_light"
                android:visibility="gone"
                android:text="Please select your Delivery Address"/>
                <LinearLayout
                    android:id="@+id/layoutPri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:visibility="visible"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/btnAddPri"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:checked="true"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="vertical" >
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:background="@color/colorLogoBlue">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:text="@string/primary_address"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/white"
                                android:textStyle="bold"
                                android:textSize="@dimen/text_size"
                                android:padding="2dp" />
                            <TextView
                                android:id="@+id/txtEditPAdd"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="10dp"
                                android:textColor="@color/white"
                                android:textStyle="bold"
                                android:text="@string/edit"
                                android:textSize="@dimen/text_size"
                                android:padding="2dp" />
                        </RelativeLayout>
                        <TableLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:background="@android:color/transparent"
                            android:layout_margin="15dp">
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginBottom="10dp" >
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/user_name"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size"
                                    android:layout_gravity="center_vertical" />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="4dp"
                                    android:paddingRight="4dp"
                                    android:text="@string/colon"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size"
                                    android:layout_gravity="center_vertical" />
                                <EditText
                                    android:id="@+id/txtName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.1"
                                    android:paddingLeft="4dp"
                                    android:paddingRight="4dp"
                                    android:hint="Enter Your Name"
                                    android:maxLength="30"
                                    android:inputType="textCapCharacters"
                                    android:textSize="@dimen/text_size"
                                    android:textColor="@android:color/black"
                                    android:background="@drawable/my_edittext_shape"/>
                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginBottom="10dp">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size"
                                    android:text="@string/user_email_id"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size"
                                    android:text="@string/colon"/>
