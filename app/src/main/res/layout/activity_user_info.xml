                                    android:layout_height="wrap_content"
                                    android:paddingLeft="4dp"
                                    android:paddingRight="4dp"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size"
                                    android:layout_gravity="center_vertical"
